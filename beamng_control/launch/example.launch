<launch>
    <arg name="scenario" default="simple_scenario_pickup" doc="[simple_scenario, weather,west_coast,simple_scenario]"/>
    <include file="$(find beamng_control)/launch/bridge.launch">
        <!-- substitute with the ip address of your target machine-->
        <arg name="host" value="10.140.0.27"/>
        <!-- <arg name="host" value="192.168.0.113"/> -->
        <!-- choose scenario-->
        <arg name="scenario_config" value="$(find beamng_control)/config/scenarios/$(arg scenario).json"/>
    </include>
</launch>
